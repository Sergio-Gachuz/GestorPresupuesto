<form action="/contrato/nuevo" method="POST">

    <label for="no_contrato">Numero de Contrato</label>
    <input class="" type="text" name="no_contrato">

    <label for="suma_total">Suma Total</label>
    <input class="" type="text" name="suma_total" required>

    <label for="vigencia">Vigencia</label>
    <input class="" type="date" name="vigencia">

    <label for="tipo_id">Tipo de contrato</label>
    <input class="" type="text" name="tipo_id">

    <label for="proveedor_id">Proveedor</label>
    <input class="" type="text" name="proveedor_id">

    <label for="partida_id">Numero de partida</label>
    <input class="" type="text" name="partida_id">

    <button>Guardar</button>
</form>